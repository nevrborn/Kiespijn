<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/fragment_result_page"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorGrey"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.bnnvara.kiespijn.ResultPage.ResultActivity">

    <include layout="@layout/dilemma_viewholder"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="24dp"
        android:gravity="fill_horizontal"
        >

        <TextView
            android:id="@+id/text_view_result_answer_A"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginRight="8dp"
            android:layout_weight="0.5"
            android:background="@color/colorYellow"
            android:gravity="center"
            android:padding="4dp"
            android:text="Opie A AAA AAAA"
            android:textAlignment="gravity"
            android:textColor="@color/colorGreen"
            />

        <TextView
            android:id="@+id/text_view_result_answer_B"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginLeft="8dp"
            android:layout_weight="0.5"
            android:background="@color/colorYellow"
            android:gravity="center"
            android:padding="4dp"
            android:text="Optie B BBB BBBB"
            android:textAlignment="gravity"
            android:textColor="@color/colorGreen"
            />

    </LinearLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        >

        <RelativeLayout
            android:id="@+id/relative_layout_middle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"
            >

            <TextView
                android:id="@+id/text_view_results_total"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="3dp"
                android:text="Alles (54)"
                android:textSize="14dp"
                android:textStyle="bold"
                />

            <TextView
                android:id="@+id/text_view_results_men"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/text_view_results_total"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="21dp"
                android:text="Mannen (18)"
                android:textSize="14dp"
                android:textStyle="bold"
                />

            <TextView
                android:id="@+id/text_view_results_women"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/text_view_results_men"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="20dp"
                android:text="Vrouwen (26)"
                android:textSize="14dp"
                android:textStyle="bold"
                />

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_toLeftOf="@id/relative_layout_middle"
            >

            <RelativeLayout
                android:id="@+id/relative_layout_total_left"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="16dp"
                >

                <com.akexorcist.roundcornerprogressbar.RoundCornerProgressBar
                    android:id="@+id/progressBar_total_left"
                    android:layout_width="match_parent"
                    android:layout_height="24dp"
                    android:orientation="horizontal"
                    app:rcBackgroundColor="@color/colorGrey"
                    app:rcBackgroundPadding="0dp"
                    app:rcProgress="63"
                    app:rcProgressColor="@color/colorRed"
                    app:rcRadius="6dp"
                    app:rcReverse="true"/>

                <TextView
                    android:id="@+id/text_view_result_total_left"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="4dp"
                    android:text="63%"
                    android:textColor="@android:color/white"
                    android:textSize="16dp"
                    android:textStyle="bold"
                    />

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/relative_layout_men_left"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/relative_layout_total_left"
                android:layout_marginBottom="16dp"
                >

                <com.akexorcist.roundcornerprogressbar.RoundCornerProgressBar
                    android:id="@+id/progressBar_men_left"
                    android:layout_width="match_parent"
                    android:layout_height="24dp"
                    android:orientation="horizontal"
                    app:rcBackgroundColor="@color/colorGrey"
                    app:rcBackgroundPadding="0dp"
                    app:rcProgress="75"
                    app:rcProgressColor="@color/colorRed"
                    app:rcRadius="6dp"
                    app:rcReverse="true"/>

                <TextView
                    android:id="@+id/text_view_result_men_left"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="4dp"
                    android:text="75%"
                    android:textColor="@android:color/white"
                    android:textSize="16dp"
                    android:textStyle="bold"
                    />

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/relative_layout_women_left"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/relative_layout_men_left"
                android:layout_marginBottom="16dp"
                >

                <com.akexorcist.roundcornerprogressbar.RoundCornerProgressBar
                    android:id="@+id/progressBar_women_left"
                    android:layout_width="match_parent"
                    android:layout_height="24dp"
                    android:orientation="horizontal"
                    app:rcBackgroundColor="@color/colorGrey"
                    app:rcBackgroundPadding="0dp"
                    app:rcProgress="55"
                    app:rcProgressColor="@color/colorRed"
                    app:rcRadius="6dp"
                    app:rcReverse="true"/>

                <TextView
                    android:id="@+id/text_view_result_women_left"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="4dp"
                    android:text="55%"
                    android:textColor="@android:color/white"
                    android:textSize="16dp"
                    android:textStyle="bold"
                    />

            </RelativeLayout>

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@id/relative_layout_middle"
            >

            <RelativeLayout
                android:id="@+id/relative_layout_total_right"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="16dp"
                >

                <com.akexorcist.roundcornerprogressbar.RoundCornerProgressBar
                    android:id="@+id/progressBar_total_right"
                    android:layout_width="match_parent"
                    android:layout_height="24dp"
                    android:orientation="horizontal"
                    app:rcBackgroundColor="@color/colorGrey"
                    app:rcBackgroundPadding="0dp"
                    app:rcProgress="37"
                    app:rcProgressColor="@color/colorGreen"
                    app:rcRadius="6dp"
                    app:rcReverse="false"/>

                <TextView
                    android:id="@+id/text_view_result_total_right"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="4dp"
                    android:text="37%"
                    android:textColor="@android:color/white"
                    android:textSize="16dp"
                    android:textStyle="bold"
                    />

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/relative_layout_men_right"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/relative_layout_total_right"
                android:layout_marginBottom="16dp"
                >

                <com.akexorcist.roundcornerprogressbar.RoundCornerProgressBar
                    android:id="@+id/progressBar_men_right"
                    android:layout_width="match_parent"
                    android:layout_height="24dp"
                    android:orientation="horizontal"
                    app:rcBackgroundColor="@color/colorGrey"
                    app:rcBackgroundPadding="0dp"
                    app:rcProgress="25"
                    app:rcProgressColor="@color/colorGreen"
                    app:rcRadius="6dp"
                    app:rcReverse="false"/>

                <TextView
                    android:id="@+id/text_view_result_men_right"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="4dp"
                    android:text="25%"
                    android:textColor="@android:color/white"
                    android:textSize="16dp"
                    android:textStyle="bold"
                    />

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/relative_layout_women_right"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/relative_layout_men_right"
                android:layout_marginBottom="16dp"
                >

                <com.akexorcist.roundcornerprogressbar.RoundCornerProgressBar
                    android:id="@+id/progressBar_women_right"
                    android:layout_width="match_parent"
                    android:layout_height="24dp"
                    android:orientation="horizontal"
                    app:rcBackgroundColor="@color/colorGrey"
                    app:rcBackgroundPadding="0dp"
                    app:rcProgress="45"
                    app:rcProgressColor="@color/colorGreen"
                    app:rcRadius="6dp"
                    app:rcReverse="false"/>

                <TextView
                    android:id="@+id/text_view_result_women_right"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="4dp"
                    android:text="45%"
                    android:textColor="@android:color/white"
                    android:textSize="16dp"
                    android:textStyle="bold"
                    />

            </RelativeLayout>

        </RelativeLayout>

    </RelativeLayout>

